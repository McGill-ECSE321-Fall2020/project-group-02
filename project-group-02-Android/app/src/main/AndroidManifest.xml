<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectgroup02">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projectgroup02Android">
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar"></activity>
        <activity
            android:name=".ShoppingCartActivity"
            android:label="@string/title_activity_shopping_cart"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar" />
        <activity
            android:name=".CheckoutActivity"
            android:label="@string/title_activity_checkout"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar" />
        <activity
            android:name=".BrowseCollectionsPageActivity"
            android:label="@string/title_activity_browse_collections_page"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar" />
        <activity
            android:name=".ItemsPageActivity"
            android:label="@string/title_activity_item_page"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar" />
        <activity android:name=".CreateCollectionActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Projectgroup02Android.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>